{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node",
      "config": {
        "NODE_ENV": "production",
        "PRISMA_GENERATE": "true"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": [
        "OPTIONS",
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ]
    }
  ]
}
